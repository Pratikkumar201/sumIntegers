import java.util.Scanner;

public class addition {
    public static void main(String[] args)
    {
        int i,j,sum=0;
        for(i=0;i<5;i++)
        {
            System.out.print("Enter numbers: ");
            Scanner s= new Scanner(System.in);
            j= s.nextInt();
            sum=sum+j;
        }
        System.out.print("Sum of given numbers is : " +sum);
    }
}
